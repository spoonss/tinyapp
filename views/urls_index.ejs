<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
    <header>
        <% include ./partials/_header.ejs %>
    </header>
    <h1>List of URLs</h1>
    <ul>
        
        <% for (let key in urls) { %>
            <% if (urls[key].userID === loggedIn){ %>
            <li><%= key %> --> <%= urls[key].longURL%>
                <span></span><form action="/urls/<%= key %>">
                    <input type="submit" value="Edit" />
                </form></span>
                <span><form action="/urls/<%= key %>/delete" method="POST" >
                    <input type="submit" value="Delete">
                </form></span>
               
            </li>
            <% } %>
        <% } %>
    </ul>


</body>
</html>


